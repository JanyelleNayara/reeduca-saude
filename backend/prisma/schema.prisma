
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../src/database/database.db"
}


model User {
  @@map("users")
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}